<?xml version="1.0" encoding="UTF-8"?>
<matrixGameEngine:GameLogic
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:matrixGameEngine="http://www.example.org/matrixGameEngine"
    multiplayer="true">
  <player Score="1"/>
  <player/>
  <map amountOfTiles="9"
      mapSizeX="3"
      mapSizeY="3"
      tileSizeY="50"
      tileSizeX="50">
    <tile Color="Red"
        type="INTERACTIVE"/>
    <tile Color="Black"
        xCoordinate="1"
        type="INTERACTIVE"/>
    <tile Color="White"
        xCoordinate="2"
        type="INTERACTIVE"/>
    <tile Color="White"
        yCoordinate="1"
        type="INTERACTIVE"/>
    <tile Color="Red"
        xCoordinate="1"
        yCoordinate="1"
        type="INTERACTIVE"/>
    <tile Color="White"
        xCoordinate="2"
        yCoordinate="1"
        type="INTERACTIVE"/>
    <tile Color="Black"
        yCoordinate="2"
        type="INTERACTIVE"/>
    <tile Color="White"
        xCoordinate="1"
        yCoordinate="2"
        type="INTERACTIVE"/>
    <tile Color="Red"
        xCoordinate="2"
        yCoordinate="2"
        type="INTERACTIVE"/>
  </map>
</matrixGameEngine:GameLogic>
